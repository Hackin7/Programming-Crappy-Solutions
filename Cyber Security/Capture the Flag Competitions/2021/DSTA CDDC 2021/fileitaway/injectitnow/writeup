#include <stdio.h>
#include <string.h>
#include <stdlib.h>                                                         #include <unistd.h>                                                         
                                                                            int check_cmd(char* command)                                                {
  int ret = 0;
                                                                              for (int i = 0; command[i] != '\0'; i++)                                    {
        if (command[i] == ' ') ret++;                                         }                                                                                                                                                       ret += strstr(command, "flag")!=0;                                          ret += strstr(command, "sh")!=0;                                            ret += strstr(command, "s\\h")!=0;                                          ret += strstr(command, "tmp")!=0;
  ret += strstr(command, "cat")!=0;
                                                                              return ret;
}                                                                                                                                                       
int main(int argc, char* argv[], char** envp)
{
  if (argc < 2) return 0;                                                     if (check_cmd(argv[1])) return 0;

  setreuid(geteuid(), geteuid());

  putenv("PATH=/whereami");
  chdir("/tmp");
                                                                              system(argv[1]);

  return 0;
}
